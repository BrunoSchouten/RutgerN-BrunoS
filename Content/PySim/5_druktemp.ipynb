    "def temperature(particles) -> float:\n",
    "    k = 1.380649e-23  \n",
    "\n",
    "    if len(particles) == 0:\n",
    "        return 0.0\n",
    "    f = len(particles[0].v)\n",
    "\n",
    "    m = particles[0].m\n",
    "    v2_sum = 0.0\n",
    "    for p in particles:\n",
    "        v2 = 0.0\n",
    "        for vi in p.v:\n",
    "            v2 += vi**2\n",
    "        v2_sum += v2\n",
    "\n",
    "    v2_avg = v2_sum / len(particles)\n",
    "\n",
    "    # temperatuur\n",
    "    temp = m * v2_avg / (f * k)\n",
    "\n",
    "    return temp"
