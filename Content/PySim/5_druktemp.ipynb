    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Constanten\n",
    "BOX_SIZE_0 = 10.0\n",
    "N = 40\n",
    "V_0 = 5.0\n",
    "RADIUS = 0.2\n",
    "DT = 0.01\n",
    "\n",
    "# Klasse en basis functies\n",
    "class ParticleClass:\n",
    "    def __init__(self, m, v, r, R):\n",
    "        self.m = m\n",
    "        self.v = np.array(v, dtype=float)\n",
    "        self.r = np.array(r, dtype=float)\n",
    "        self.R = R\n",
    "\n",
    "    def update_position(self):\n",
    "        self.r += self.v * DT\n",
    "\n",
    "    @property\n",
    "    def momentum(self):\n",
    "        return self.m * self.v\n",
    "\n",
    "def collide_detection(p1: ParticleClass, p2: ParticleClass) -> bool:\n",
    "    dx = p1.r[0] - p2.r[0]\n",
    "    dy = p1.r[1] - p2.r[1]\n",
    "    rr = p1.R + p2.R\n",
    "    return dx**2 + dy**2 < rr**2\n",
    "\n",
    "def particle_collision(p1: ParticleClass, p2: ParticleClass):\n",
    "    m1, m2 = p1.m, p2.m\n",
    "    delta_r = p1.r - p2.r\n",
    "    delta_v = p1.v - p2.v\n",
    "    dot_product = np.dot(delta_r, delta_v)\n",
    "    if dot_product >= 0:\n",
    "        return\n",
    "    distance_squared = np.dot(delta_r, delta_r)\n",
    "    p1.v -= 2 * m2 / (m1 + m2) * dot_product / distance_squared * delta_r\n",
    "    p2.v += 2 * m1 / (m1 + m2) * dot_product / distance_squared * delta_r\n",
    "\n",
    "# Druk-specifieke wandbotsingen\n",
    "impulse_outward = 0.0\n",
    "\n",
    "def top_down_collision(particle: ParticleClass):\n",
    "    global impulse_outward\n",
    "    if abs(particle.r[1]) + particle.R > BOX_SIZE_0 / 2:\n",
    "        particle.r[1] = np.sign(particle.r[1]) * (BOX_SIZE_0/2 - particle.R)\n",
    "        impulse_outward += abs(particle.momentum[1]) * 2   # Δp = 2 |p_y|\n",
    "        particle.v[1] *= -1\n",
    "\n",
    "def left_right_collision(particle: ParticleClass):\n",
    "    global impulse_outward\n",
    "    if abs(particle.r[0]) + particle.R > BOX_SIZE_0 / 2:\n",
    "        particle.r[0] = np.sign(particle.r[0]) * (BOX_SIZE_0/2 - particle.R)\n",
    "        impulse_outward += abs(particle.momentum[0]) * 2   # Δp = 2 |p_x|\n",
    "        particle.v[0] *= -1\n",
    "\n",
    "def handle_walls(particles):\n",
    "    global impulse_outward, pressure\n",
    "    impulse_outward = 0.0\n",
    "    for p in particles:\n",
    "        left_right_collision(p)\n",
    "        top_down_collision(p)\n",
    "    # Druk = totale uitgaande impuls / (omtrek * Δt)\n",
    "    pressure = impulse_outward / (4 * BOX_SIZE_0 * DT)\n",
    "\n",
    "# Overige hulp functies\n",
    "def create_particles(particles):\n",
    "    particles.clear()\n",
    "    for i in range(N):\n",
    "        vx = np.random.uniform(-V_0, V_0)\n",
    "        vy = np.random.choice([-1, 1]) * np.sqrt(max(0, V_0**2 - vx**2))\n",
    "        pos = np.random.uniform(-BOX_SIZE_0/2 + RADIUS, BOX_SIZE_0/2 - RADIUS, 2)\n",
    "        particles.append(ParticleClass(m=1.0, v=[vx, vy], r=pos, R=RADIUS))\n",
    "\n",
    "def handle_collisions(particles):\n",
    "    num_particles = len(particles)\n",
    "    for i in range(num_particles):\n",
    "        for j in range(i+1, num_particles):\n",
    "            if collide_detection(particles[i], particles[j]):\n",
    "                particle_collision(particles[i], particles[j])\n",
    "\n",
    "def take_time_step(particles):\n",
    "    for p in particles:\n",
    "        p.update_position()\n",
    "    handle_collisions(particles)\n",
    "    handle_walls(particles)\n",
    "\n",
    "# Simulatie en plot druk\n",
    "particles = []\n",
    "create_particles(particles)\n",
    "\n",
    "num_steps = 100\n",
    "pressures = []\n",
    "\n",
    "for step in range(num_steps):\n",
    "    take_time_step(particles)\n",
    "    pressures.append(pressure)\n",
    "\n",
    "plt.figure(figsize=(8, 5))\n",
    "plt.plot(pressures, 'r-', lw=2)\n",
    "plt.title('Druk als functie van de tijd')\n",
    "plt.xlabel('Tijdstap')\n",
    "plt.ylabel('Druk (simulatie-eenheden)')\n",
    "plt.grid(True)\n",
    "plt.show()\n",
    "\n",
    "print(f\"Gemiddelde druk: {np.mean(pressures):.4f}\")"
   ]
