   "source": [
    "from scipy.optimize import curve_fit\n",
    "\n",
    "# mass of particles \n",
    "m = 1.0  \n",
    "kB = 1.0 # 1.38*10**-23  # Boltzmann constant  \n",
    "\n",
    "# centrale binwaarden\n",
    "bin_centers = 0.5 * (bins[:-1] + bins[1:])\n",
    "\n",
    "\n",
    "# Definitie Maxwell-Boltzmann 2D verdeling functie\n",
    "def f_MB_2D(v, T):\n",
    "    return (m / (kB * T)) * v * np.exp(- m * v**2 / (2 * kB * T))\n",
    "\n",
    "# functiefit aan het histogram\n",
    "popt, pcov = curve_fit(f_MB_2D, bin_centers, norm_counts, p0=[1.0])\n",
    "\n",
    "T_fit = popt[0]\n",
    "print(\"Gefitte temperatuur T =\", T_fit)\n",
    "\n",
    "# resultaat plotten\n",
    "plt.figure()\n",
    "plt.stairs(norm_counts, bins, fill=True, label='Simulatie histogram')\n",
    "v_fit = np.linspace(0, bins[-1], 200)\n",
    "plt.plot(v_fit, f_MB_2D(v_fit, T_fit), 'r-', label=f'MB-fit, T={T_fit:.2f}')\n",
    "plt.xlabel('Snelheid v')\n",
    "plt.ylabel('Waarschijnlijkheid')\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n"
