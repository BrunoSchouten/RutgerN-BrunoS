   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# Gegevens (invullen)\n",
    "m_water = 247      # massa water in g\n",
    "m_obj = 546.4 - 247  # massa voorwerp in g\n",
    "c_obj = 0.900      # J/(g*K)\n",
    "\n",
    "T_obj_0 = 72.0     # begin temperatuur voorwerp (째C)\n",
    "T_water_0 = 19.4   # begin temperatuur water (째C)\n",
    "T_eind = 35.2      # eind temperatuur (gemeten na 10 minuten en 30 seconden, 째C)\n",
    "\n",
    "# warmteverlies/overdracht\n",
    "Q = m_obj * c_obj * (T_obj_0 - T_eind)          # opgenomen door water\n",
    "c_water = Q / (m_water * (T_eind - T_water_0))   # J/(g*K)\n",
    "\n",
    "print(f\"Soortelijke warmte van water = {c_water:.3f} J/(g*K)\")\n",
    "print(f\"In SI-eenheden = {c_water*1000:.2f} J/(kg*K)\")\n"
